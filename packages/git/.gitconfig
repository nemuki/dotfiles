[init]
	defaultBranch = main
[pull]
	rebase = true
[alias]
	vacuum = "!f () { git checkout $1; git branch --merged|egrep -v '\\*|develop|main'|xargs git branch -d; git fetch --prune; };f"
[gpg]
	program = /opt/homebrew/bin/gpg
[commit]
	gpgsign = true
[core]
	autocrlf = input
