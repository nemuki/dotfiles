[core]
	autocrlf = input
[commit]
	gpgsign = true
[gpg]
	program = /opt/homebrew/bin/gpg
[alias]
	delete-marged-branch = "!f () { git branch --merged | egrep -v '\\*|develop|main|master|release' | xargs git branch -d; git fetch --prune; };f"
[init]
	defaultBranch = main
[pull]
	rebase = true
